<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.baemongsil.ex.lesson06.dao.BookingDAO">
  
  	
  	<select id="selectBookingList"  resultType="com.baemongsil.ex.lesson06.model.Booking">
  		SELECT 
			`id`
			, `name`
			, `headcount`
			, `day`
			, `date`
			, `phoneNumber`
			, `state`
			, `createdAt`
			, `updatedAt`
		FROM
			`booking`
		ORDER BY 
			`id` DESC
  	</select>
  
  </mapper>